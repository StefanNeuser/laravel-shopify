{
    "name": "247apps/laravel-shopify",
    "description": "Shopify package for Laravel package based on the laravel-shopify package from Tyler King",
    "keywords": [
        "laravel",
        "shopify",
        "shopify-api",
        "api"
    ],
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Tyler King",
            "email": "tyler@ohmybrew.com"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^7.0.1",
        "guzzlehttp/promises": "v1.3.1",
        "laravel/framework": "^5.4 || ^6.0 || ^7.0 || ^8.0",
        "jenssegers/agent": "^2.6"
    },
    "require-dev": {
        "orchestra/database": "~3.6 | ~4.0",
        "orchestra/testbench": "~3.6 | ~4.0",
        "phpunit/phpunit": "~6.0 || ~7.0 || ~8.0",
        "squizlabs/php_codesniffer": "^3.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "OhMyBrew\\ShopifyApp\\": "src/ShopifyApp"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OhMyBrew\\ShopifyApp\\Test\\": "tests"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "laravel": {
            "providers": [
                "OhMyBrew\\ShopifyApp\\ShopifyAppProvider"
            ],
            "aliases": {
                "ShopifyApp": "OhMyBrew\\ShopifyApp\\Facades\\ShopifyApp"
            }
        }
    }
}